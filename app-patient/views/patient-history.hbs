<body>
    <nav class="ui secondary pointing menu">
        <a class="item active">Appointments</a>
        <a class="item">Requests</a>
        <a class="item">History</a>
        <div class="right menu"><a class="ui item">Logout</a></div>
    </nav>
    
    <div class="ui centered">
            <div class="ui container cards sixteen column grid centered">
                {{#each appointment}}
                   <div class="card eight wide column">
                     <b>{{date}} @{{time}}</b>
                     <div class="content">
                       <a class="header">
                           {{process}}
                       </a>   
                       <div class = "description"><b>
                        {{doctor}}</b></div>
                     </div>
                   </div>
                {{/each}}
             </div>
    </div>
    

